{
	"name": "magazine_editor",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint"
	},
	"dependencies": {
		"@ckeditor/ckeditor5-build-decoupled-document": "^19.0.2",
		"@ckeditor/ckeditor5-editor-decoupled": "^19.0.1",
		"@ckeditor/ckeditor5-vue": "^1.0.1",
		"core-js": "^3.4.3",
		"foundation-sites": "^6.6.1",
		"normalize.css": "^8.0.1",
		"sortablejs": "^1.10.1",
		"tinysort": "^3.2.5",
		"tiptap": "^1.26.6",
		"tiptap-extensions": "^1.28.6",
		"vue": "^2.6.10",
		"vue-router": "^3.1.3",
		"vue2-dropzone": "^3.6.0",
		"vuex": "^3.1.2"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "^4.1.0",
		"@vue/cli-plugin-eslint": "^4.1.0",
		"@vue/cli-service": "^5.0.8",
		"babel-eslint": "^10.0.3",
		"eslint": "^5.16.0",
		"eslint-plugin-vue": "^5.0.0",
		"vue-template-compiler": "^2.6.10"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"rules": {
			"no-console": "off",
			"no-unused-vars": [
				"error",
				{
					"vars": "all",
					"args": "after-used",
					"ignoreRestSiblings": false,
					"caughtErrors": "none"
				}
			]
		},
		"parserOptions": {
			"parser": "babel-eslint"
		}
	},
	"prettier": {
		"singleQuote": true
	},
	"browserslist": [
		"> 1%",
		"last 2 versions"
	]
}
